<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="500" Width="300" Height="500"
        x:Class="ws.MainWindow"
        Title="IWTCMS" Padding="0,0,0,0" MinHeight="500" MinWidth="300" Icon="icon.webp">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <Menu.Styles></Menu.Styles>
            <MenuItem Header="_File">
                <MenuItem Header="_New Config" Click="AddConfigMenuItemOnClick" />
                <MenuItem Header="_Quit" Click="QuitMenuItemOnClick"></MenuItem>
            </MenuItem>
            <MenuItem Header="_Help">
                <MenuItem Header="_About" Click="ShowAbout"></MenuItem>
            </MenuItem>
        </Menu>
        <TabControl x:Name="TabControl" Padding="5,0,5,5" />
    </DockPanel>


</Window>